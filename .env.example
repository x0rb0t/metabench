# Environment variables for MetaBench
# Copy this file to .env and fill in your values

# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# OpenAI API Configuration (alternative)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# Local LLM Configuration (default)
LOCAL_API_KEY=not-needed
LOCAL_BASE_URL=http://localhost:1234/v1

# Model Configuration
MODEL_NAME=gpt-4
CREATIVE_MODEL=gpt-4
STRUCTURED_MODEL=gpt-4
TRANSFORM_MODEL=gpt-4

# Benchmark Configuration
MAX_COMPLEXITY=5
TRIALS_PER_COMPLEXITY=3
TEMPERATURE=0.3
CONTENT_TYPES=code,text,data,configuration,documentation
TOPIC=artificial intelligence
